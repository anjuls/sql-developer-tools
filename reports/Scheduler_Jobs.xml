	</CustomValues>
	<query>
		<sql><![CDATA[/* Scheduler Reports DBA Scheduler Jobs */ 
SELECT * FROM "SYS"."DBA_SCHEDULER_JOBS"
 WHERE :JOB_NAME IS NULL OR INSTR( UPPER( JOB_NAME ), UPPER( :JOB_NAME ) ) > 0
 ORDER BY last_start_date DESC NULLS LAST]]></sql>
